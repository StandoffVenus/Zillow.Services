<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>Provides .NET Std API for Zillow services</Description>
    <Copyright />
    <PackageProjectUrl>https://github.com/outbred/Zillow.Services</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <RepositoryUrl>https://github.com/outbred/Zillow.Services</RepositoryUrl>
    <Authors>Anthony Massaro, Brent Bulla</Authors>
    <Company />
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <ApplicationIcon>zillow.ico</ApplicationIcon>
    <PackageIcon>zillow.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="ZillowClient.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

	<ItemGroup>
		<None Include="zillow.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
